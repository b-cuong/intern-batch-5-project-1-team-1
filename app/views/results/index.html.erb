<main class="py-4">
  <div class="container">
    <div class="row justify-content-center">
      <div class="col-md-12">
        <div class="card">
          <div class="card-body">
            <h4><%= t(".result") %></h4>
            <div class="row">
              <div class="col-md-12">
                <p class="text-danger"><%= t(".unsuitable") %>
                  ()
                </p>
              </div>
              <div class="col-md-8">
                <table class="table table-bordered table-sm">
                  <thead>
                    <tr>
                      <th scope="col">#</th>
                      <th scope="col"><%= t(".group_character") %></th>
                      <th scope="col"><%= t(".score") %></th>
                    </tr>
                  </thead>
                  <tbody>
                    <tr>
                      <th scope="row"></th>
                      <td></td>
                      <td></td>
                    </tr>
                  </tbody>
                </table>
              </div>
            </div>
            <h4><%= t(".conclusion") %></h4>
            <%= render "results/character" %>
            <div class="form-group">
              <a href="#" class="btn btn-danger btn-xs btn-block">
                <%= t(".remake") %>
              </a>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</main>
