<tr>
  <td><%= question.id %> </td>
  <td><%= question.content %> </td>
  <td><%= question.character.name %></td>
  <td scope="row">
    <div>
      <%= link_to t(".edit"), edit_question_path(question), class: 'btn btn-sm btn-primary' %>
      <%= link_to t(".delete"), question, method: :delete, data: { confirm: t(".delete") } %>
    </div>
  </td>
</tr>
